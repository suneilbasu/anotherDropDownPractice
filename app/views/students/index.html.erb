<h1>Students#index</h1>
<%= form_tag students_path, :method => 'get' do %>
  <%= text_field_tag :search, params[:search] %>
  <%= collection_select(:student, :tutor_id, Tutor.all, :id, :name, prompt: true) %>
  <%= submit_tag "Search" %>
<% end %>
<% @students.each do |n| %>
<li> <%= n.fname %> <%= n.sname %> <%= n.tutor.name %> </li>
<% end %>
